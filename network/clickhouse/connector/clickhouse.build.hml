kind: ConnectorManifest
version: v1
spec:
  supergraphManifests:
    - base
definition:
  name: clickhouse
  type: cloud
  connector:
    type: hub
    name: hasura/clickhouse:v0.2.5
  deployments:
    - context: .
      env:
        CLICKHOUSE_PASSWORD:
          valueFromEnv: CLICKHOUSE_PASSWORD
        CLICKHOUSE_URL:
          valueFromEnv: CLICKHOUSE_URL
        CLICKHOUSE_USERNAME:
          valueFromEnv: CLICKHOUSE_USERNAME
